        "source_ref": "Accord franco-algérien 27/12/1968 art. 7",
        "conditions": {
            "AND": [
                {
                    "var": "family.sponsor_nationality",
                    "op": "EQ",
                    "val": "ALGERIAN"
                },
                {
                    "var": "family.is_polygamous",
                    "op": "EQ",
                    "val": false
                },
                {
                    "var": "family.rf_has_valid_titre_sejour",
                    "op": "EQ",
                    "val": true
                },
                {
                    "var": "family.rf_marital_status",
                    "op": "EQ",
                    "val": "MARRIED"
                },
                {
                    "OR": [
                        {
                            "var": "family.presence_duration",
                            "op": "EQ",
                            "val": "MORE_18"
                        },
                        {
                            "var": "family.presence_duration",
                            "op": "EQ",
                            "val": "12_18"
                        }
                    ]
                },
                {
                    "OR": [
                        {
                            "var": "family.has_handicap_allowance",
                            "op": "EQ",
                            "val": true
                        },
                        {
                            "AND": [
                                {
                                    "var": "work.salary_monthly_gross",
                                    "op": "GTE",
                                    "val": "@config:financial_thresholds.salary_monthly_gross.regroupement_familial_resources"
                                },
                                {
                                    "var": "family.income_source",
                                    "op": "NEQ",
                                    "val": "RSA_ALOWANCE"
                                }
                            ]
                        }
                    ]
                },
                {
                    "var": "family.housing_status",
                    "op": "EQ",
                    "val": "OWNED_RENTED"
                },
                {
                    "var": "family.rf_family_members_count",
                    "op": "GTE",
                    "val": 1
                }
            ]
        },
        "tier": "PREMIUM",
        "duration_years": 1,
        "gives_work_right": true,
        "leads_to_naturalisation": true,
        "request_type": "FIRST_OR_RENEWAL",
        "documents": [
            "PASSPORT",
            "RESIDENCE_PROOF",
            "TAX_NOTICE",
            "MARRIAGE_CERTIFICATE",
            "BIRTH_CERTIFICATE",
            "BANK_STATEMENTS",
            "TITRE_SEJOUR_ACTUEL",
            "COMMUNITY_PROOF"
        ],
        "note": "Durée de résidence réduite à 12 mois pour les Algériens"
